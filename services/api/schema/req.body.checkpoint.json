{
    "type": "object",
    "required": [
        "name",
        "classes"
    ],
    "additionalProperties": false,
    "properties": {
        "name": {
            "type": "string"
        },
        "analytics": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "counts",
                "percent",
                "f1score"
            ],
            "properties": {
                "counts": {
                    "type": "number"
                },
                "percent": {
                    "type": "number"
                },
                "f1score": {
                    "type": "number"
                }
            }
        },
        "classes": {
            "type": "array",
            "description": "Named output classes & their associated colours",
            "items": {
                "type": "object",
                "additionalProperties": false,
                "required": [
                    "name",
                    "color"
                ],
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "color": {
                        "type": "string"
                    }
                }
            }
        },
        "geoms": {
            "type": "array",
            "description": "Array of GeoJSON Multipoint geometries that were used for retraining. Length of geomms array must equals classes",
            "items": {
                "type": ["null", "object"]
            }
        }
    }
}
